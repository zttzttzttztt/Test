class TestTestdemo():
  def setup_method(self, method):
    self.driver = webdriver.Firefox()
    self.vars = {}

  def teardown_method(self, method):
    self.driver.quit()

  def test_testdemo(self):
    self.driver.get("http://127.0.0.1/")
    self.driver.set_window_size(1028, 704)
    self.driver.find_element(By.ID, "zentaobiz").click()
    self.driver.find_element(By.ID, "account").send_keys("admin")
    self.driver.find_element(By.NAME, "password").click()
    self.driver.find_element(By.NAME, "password").send_keys("ZTTztt970909")
    self.driver.find_element(By.ID, "submit").click()




